menu "window_menu"
{
    # "Window Menu"         f.title

    "Identify"              f.identify

    "Move To"               f.menu "window_move_to"
    "Occupy"                f.occupy

    "Refresh"               f.winrefresh
    "Iconify"               f.iconify
    "Resize"                f.resize
    "Lower"                 f.lower
    "Move"                  f.move
    "Focus"                 f.focus
    "Raise"                 f.raise
    "Toggle AutoRaise"      f.AutoRaise

    "Kill"                  f.destroy
    "Dismiss"               f.delete
}

menu "window_move_to"
{
    "1"     f.function "move-to-#1"
    "2"     f.function "move-to-#2"
    "3"     f.function "move-to-#3"
    "4"     f.function "move-to-#4"
    "5"     f.function "move-to-#5"
    "6"     f.function "move-to-#6"
}
