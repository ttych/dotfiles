#--------------------
# Functions
#--------------------

function "move-or-iconify"	{ f.move f.deltastop f.iconify }
function "raise-resize"		{ f.raise f.resize }
function "lower-resize"		{ f.lower f.resize }
function "raise-move"		{ f.raise f.move }
function "lower-move"		{ f.lower f.move }
function "raiselower-move"		{ f.raiselower f.move }
function "raiselower-resize"		{ f.raiselower f.resize }

Function "move-to-#1"		{ f.addtoworkspace "#1"	f.vanish }
Function "move-to-#2"		{ f.addtoworkspace "#2"	f.vanish }
Function "move-to-#3"		{ f.addtoworkspace "#3"	f.vanish }
Function "move-to-#4"		{ f.addtoworkspace "#4"	f.vanish }
Function "move-to-#5"		{ f.addtoworkspace "#5"	f.vanish }
Function "move-to-#6"		{ f.addtoworkspace "#6"	f.vanish }
Function "move-to-#7"		{ f.addtoworkspace "#7"	f.vanish }
Function "move-to-#8"		{ f.addtoworkspace "#8"	f.vanish }
Function "move-to-#9"		{ f.addtoworkspace "#9"	f.vanish }

Function "move-with-#1"		{ f.addtoworkspace "#1"	f.vanish  f.gotoworkspace "#1" }
Function "move-with-#2"		{ f.addtoworkspace "#2"	f.vanish  f.gotoworkspace "#2" }
Function "move-with-#3"		{ f.addtoworkspace "#3"	f.vanish  f.gotoworkspace "#3" }
Function "move-with-#4"		{ f.addtoworkspace "#4"	f.vanish  f.gotoworkspace "#4" }
Function "move-with-#5"		{ f.addtoworkspace "#5"	f.vanish  f.gotoworkspace "#5" }
Function "move-with-#6"		{ f.addtoworkspace "#6"	f.vanish  f.gotoworkspace "#6" }
Function "move-with-#7"		{ f.addtoworkspace "#7"	f.vanish  f.gotoworkspace "#7" }
Function "move-with-#8"		{ f.addtoworkspace "#8"	f.vanish  f.gotoworkspace "#8" }
Function "move-with-#9"		{ f.addtoworkspace "#9"	f.vanish  f.gotoworkspace "#9" }

