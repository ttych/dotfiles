Button3 = m2	: title		: f.menu "window_menu"

Button1 =	: frame         : f.resize
Button1 = m2  	: frame		: f.resize

Button1 = m	: window	: f.function "raiselower-move"
Button1 = m|m2	: window	: f.function "raiselower-move"
Button3 = m	: window	: f.resize
Button3 = m|m2	: window	: f.resize

Button1 = c|m	: window	: f.iconify
Button1 = c|m|m2: window	: f.iconify


